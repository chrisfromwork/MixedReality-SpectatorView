    <file src="..\..\NuGet\Content\SpatialAlignment.ASA\**" target="MRSV\content\SpatialAlignment.ASA\"
      exclude="**\*.csproj;**\*.csproj.meta;**\*.sln;**\*.sln.meta;**\*.md;**\*.md.meta" />
    <file src="..\..\NuGet\Content\SpatialAlignment.ASA.Editor\**" target="MRSV\content\SpatialAlignment.ASA.Editor\"
      exclude="**\*.csproj;**\*.csproj.meta;**\*.sln;**\*.sln.meta;**\*.md;**\*.md.meta" />
