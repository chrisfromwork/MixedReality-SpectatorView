      exclude="**\*.csproj;**\*.csproj.meta;**\*.sln;**\*.sln.meta;**\*.md;**\*.md.meta" />

    <!-- Compiled binaries -->
    <file src="..\..\NuGet\Plugins\AndroidPlayer\**" target="MRSV\bin\Player\Android\" exclude="**\*Unity*"/>
    <file src="..\..\NuGet\Plugins\iOSPlayer\**" target="MRSV\bin\Player\iOS\" exclude="**\*Unity*"/>
    <file src="..\..\NuGet\Plugins\WSAPlayer\**" target="MRSV\bin\Player\WSA\" exclude="**\*Unity*"/>
